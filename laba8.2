function x=anny(a,b)
e=(b-a)/2;
  while e>=0.001
    y1=3*sin(a)+5*cos(a)
    y2=3*sin(b)+5*cos(b)
      if y1*y2 >0 then x='нет корней',end
    x=(a+b)/2;
    y=3*sin(x)+5*cos(x);
      if y==0 then disp('точный корень'),
              elseif y1*y<0 then b=x,
                             else a=x,
      end
e=(b-a)/2;
end
disp(x,'x=')
endfunction

